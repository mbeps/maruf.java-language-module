{
    "name": "java-extension-pack",
    "displayName": "Java Extension Pack",
    "description": "Includes only important Java extensions and nothing else. ",
    "version": "0.0.1",
    "publisher": "Maruf",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Extension Packs"
    ],
    "repository": {
		"type": "git",
		"url": "https://github.com/mbeps/java-extension-pack"
	},
    "activationEvents": [
        "onLanguage:python"
    ],
    "extensionPack": [
        "redhat.java",
		"vscjava.vscode-java-debug",
		"vscjava.vscode-java-dependency",
		"vscjava.vscode-java-test",
		"vscjava.vscode-maven",
		"shengchen.vscode-checkstyle", 
		"dhruv.maven-dependency-explorer", 

        "vscjava.vscode-gradle", 
		"naco-siren.gradle-language"
    ]
}